{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Web",
  "extends": "./base.json",
  "compilerOptions": {
    "allowJs": true
  },
  "declaration": false,
  "declarationMap": false,
  "incremental": true,
  "jsx": "preserve",
  "lib": [
    "dom",
    "dom.iterable",
    "esnext"
  ],
  "module": "esnext",
  "noEmit": true,
  "resolveJsonModule": true,
  "strict": false,
  "target": "es5",
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}